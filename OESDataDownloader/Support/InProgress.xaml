        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:support="clr-namespace:OESDataDownloader.Support"
        mc:Ignorable="d"
        Title="InProgress" Height="115" Width="100
        " WindowStartupLocation="CenterOwner" ResizeMode="NoResize" WindowStyle="None" Opacity="1">
